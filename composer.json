{
  "name": "vaclavvanik/xml-to-array",
  "description": "Convert xml to array",
  "license": "MIT",
  "type": "library",
  "homepage": "https://github.com/vaclavvanik/xml-to-array",
  "keywords": [
    "convert",
    "array",
    "xml"
  ],
  "authors": [
    {
      "name": "Václav Vaník",
      "email": "vaclav.vanik@gmail.com"
    }
  ],
  "config": {
    "sort-packages": true
  },
  "require": {
    "php": "^7.3 || ^8.0",
    "ext-dom": "*",
    "ext-libxml": "*"
  },
  "require-dev": {
    "laminas/laminas-coding-standard": "^2.1.4",
    "phpunit/phpunit": "^9.5.0"
  },
  "autoload": {
    "psr-4": {
      "VaclavVanik\\XmlToArray\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "VaclavVanikTest\\XmlToArray\\": "test/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.2.x-dev"
    }
  },
  "scripts": {
    "check": [
      "@cs-check",
      "@test"
    ],
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "test": "vendor/bin/phpunit --colors=always",
    "test-coverage-html": "XDEBUG_MODE=coverage vendor/bin/phpunit --coverage-html reports/codecoverage"
  }
}
